{
  "expo": {
    "name": "JourneyBuddy",
    "slug": "JourneyBuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tiagomvalente.JourneyBuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tiagomvalente.JourneyBuddy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow JourneyBuddy to access your photos",
          "cameraPermission": "Allow JourneyBuddy to access your camera",
          "allowsEditing": true,
          "quality": 0.8
        }
      ], [
        "expo-media-library",
        {
          "photosPermission": "Allow Journey Buddy to access your photos.",
          "savePhotosPermission": "Allow Journey Buddy to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "95fb7f1c-da64-441c-a514-37f0fc032a0c"
      }
    }
  }
}
